{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://url-shortener-cbtn.onrender.com/api/:path*"
    },
    {
      "source": "/:code([a-zA-Z0-9]+)",
      "destination": "https://url-shortener-cbtn.onrender.com/:code"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
