{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://urlshort-backend.loca.lt/api/:path*"
    },
    {
      "source": "/:code([a-zA-Z0-9]+)",
      "destination": "https://urlshort-backend.loca.lt/:code"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
